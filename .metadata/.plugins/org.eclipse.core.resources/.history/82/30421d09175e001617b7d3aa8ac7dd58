package bean;

public class Customer
{
	private int accNo;
	private String address;
	private long phone;
	private String occupation;
	
	public Customer(int accNo, String address, long phone, String occupation) 
	{
	
		this.accNo = accNo;
		this.address = address;
		this.phone = phone;
		this.occupation = occupation;
	}
	
	
	

}
